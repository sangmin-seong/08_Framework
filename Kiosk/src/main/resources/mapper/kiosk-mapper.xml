<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<!-- 연결할 Mapper 인터페이스 파일 경로 작성 (필수)  -->
<mapper namespace="edu.kh.kiosk.mapper.KioskMapper">
	
	<select id="selectAll"
			resultType="Kiosk">
			SELECT
				MENU_NO,
				MENU_TITLE,
				MENU_PRICE,
				MENU_RESIDUAL,
				ENROLL_DATE
			FROM TB_MENU
	</select>
	
</mapper>
